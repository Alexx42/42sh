sequence=ls ../../ -R | grep ".txt" > text_files;
sequence=setenv TRUC toto ; echo ${TRUC} ; echo ${truc}; setenv TRUC titi ; echo ${TRUC} ; echo ${sdse}
sequence=ls > toto ; date >> toto ; >> toto ls ; < toto cat ;
sequence=ls /dev | grep tty | sort -r | rev > toto ; < toto cat | rev | wc -l > titi
sequence=env env env env env env ls && env env env ls && env ls && ls && env env env env env env env env env env env env `echo ls` && ls && env env env env env env env env env env env env env env env ls

